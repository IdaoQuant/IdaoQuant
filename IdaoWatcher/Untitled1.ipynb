   "source": [
    "from multiprocessing.pool import ThreadPool\n",
    "scale = 10\n",
    "items_before = [(0,3), (1,8), (2,9), (3,5), (5,12)]\n",
    "def process(x, y):\n",
    "    print(str(x) + '->' + str(scale * x))\n",
    "    return scale * y\n",
    "\n",
    "p = ThreadPool()\n",
    "items_after = p.starmap(process, items_before)\n",
    "items_after"
   ]
